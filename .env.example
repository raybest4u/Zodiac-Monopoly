# 规则优先级配置
# Rule Priority Configuration
RULE_PRIORITY_CRITICAL=100
RULE_PRIORITY_HIGH=95
RULE_PRIORITY_NORMAL=85
RULE_PRIORITY_LOW=75
RULE_PRIORITY_BACKGROUND=50

# 事件集成配置
# Event Integration Configuration
EVENT_INTEGRATION_TIMEOUT=5000
